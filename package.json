{
  "name": "story-begin-pwa",
  "version": "2.0.0",
  "description": "Progressive Web App untuk berbagi cerita dengan fitur offline dan push notification",
  "main": "src/scripts/index.js",
  "scripts": {
    "start": "webpack serve --config webpack.dev.js --open",
    "build": "webpack --config webpack.prod.js",
    "build-dev": "webpack --config webpack.dev.js",
    "serve": "http-server dist -p 9000 -c-1",
    "lint": "eslint src/scripts/**/*.js",
    "lint:fix": "eslint src/scripts/**/*.js --fix"
  },
  "keywords": [
    "pwa",
    "progressive-web-app",
    "story-sharing",
    "offline-first",
    "push-notification",
    "indexeddb",
    "service-worker",
    "dicoding-submission"
  ],
  "author": "Alya Siti Rahmah",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "html-webpack-plugin": "^5.5.3",
    "http-server": "^14.1.1",
    "mini-css-extract-plugin": "^2.7.6",
    "style-loader": "^3.3.3",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "leaflet": "^1.9.4"
  },
  "browserslist": ["> 1%", "last 2 versions", "not dead"],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/story-begin-pwa.git"
  },
  "bugs": {
    "url": "https://github.com/username/story-begin-pwa/issues"
  },
  "homepage": "https://username.github.io/story-begin-pwa"
}
