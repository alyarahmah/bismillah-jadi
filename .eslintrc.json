{
  "env": {
    "browser": true,
    "es2021": true,
    "serviceworker": true
  },
  "extends": ["eslint:recommended"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "linebreak-style": ["error", "windows"],
    "quotes": ["error", "double"],
    "semi": ["error", "never"],
    "no-unused-vars": "warn",
    "no-console": "off",
    "no-undef": "error"
  },
  "globals": {
    "L": "readonly"
  }
}
